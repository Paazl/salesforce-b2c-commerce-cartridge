<iscontent type="application/json" charset="UTF-8" compact="true"/>
<iscomment>
	This template renders the Street name and City based on postcode and street number provided, as JSON response. or an empty JSON object
</iscomment>
<isscript>	 
	var o = {};
	if (!empty(pdict.addressValidationResponse)) {
		o = pdict.addressValidationResponse;		
	}
	// serialize to json string
	var ojson = JSON.stringify(o);
</isscript>
<isprint value="${ojson}" encoding="off"/>