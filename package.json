{
  "name": "int_paazl_core",
  "version": "20.1.0",
  "description": "Paazl plugin for Storefront Reference Architecture",
  "scripts": {
    "test": "sgmf-scripts --test test/unit/**/*.js",
    "test:integration": "sgmf-scripts --integration 'test/integration/**/*.js'",
    "lint": "sgmf-scripts --lint js && sgmf-scripts --lint css",
    "compile:scss": "sgmf-scripts --compile css",
    "compile:js": "sgmf-scripts --compile js",
    "watch": "sgmf-scripts --watch",
    "uploadCartridge": "sgmf-scripts --uploadCartridge int_paazl_core && sgmf-scripts --uploadCartridge modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SalesforceCommerceCloud/int_paazl.git"
  },
  "author": "Clockwork <info@clockwork.nl>",
  "license": "MIT",
  "homepage": "https://github.com/SalesforceCommerceCloud/int_paazl",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-subset": "^1.5.0",
    "cleave.js": "^1.0.1",
    "cheerio": "0.22.0",
    "css-loader": "^0.28.11",
    "eslint": "^3.2.2",
    "eslint-config-airbnb-base": "^5.0.1",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-sitegenesis": "~1.0.0",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.12.0",
    "node-sass": "^4.9.4",
    "postcss-loader": "^2.1.5",
    "properties-parser": "^0.3.1",
    "proxyquire": "1.7.4",
    "request-promise": "^4.2.2",
    "sass-loader": "^7.0.3",
    "sgmf-scripts": "^2.1.1",
    "shelljs": "^0.7.7",
    "sinon": "^1.17.4",
    "stylelint": "^7.1.0",
    "stylelint-config-standard": "^12.0.0",
    "stylelint-scss": "^1.3.4"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 10"
  ]
}
